{
  "name": "@jjangga0214/history-trigger",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~history-trigger": "dist"
  },
  "scripts": {
    "_": "dotenv -e ../../.env --",
    "build": "tsc --build tsconfig.build.json && link-module-alias",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage/lcov-report",
    "ts": "yarn _ ts-node -r tsconfig-paths/register",
    "tsd": "yarn _ ts-node-dev --respawn -r tsconfig-paths/register",
    "dev:debug": "yarn tsd src/index.ts",
    "dev": "pm2 start --restart-delay 500 --name history-trigger \"yarn ts src/index.ts\"",
    "down": "pm2 delete history-trigger",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "@jjangga0214/history-db": "^1.0.0"
  },
  "devDependencies": {}
}
