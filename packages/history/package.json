{
  "name": "@jjangga0214/history",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~history": "dist"
  },
  "scripts": {
    "_": "dotenv -e ../../.env --",
    "build": "tsc --build tsconfig.build.json && link-module-alias",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage/lcov-report",
    "ts": "yarn _ ts-node-dev --respawn -r tsconfig-paths/register",
    "dev:debug": "yarn ts src/index.ts",
    "dev": "pm2 start --restart-delay 250 --name history \"yarn dev_\"",
    "down": "pm2 delete history",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@apollo/federation": "^0.11.3",
    "apollo-server": "^2.9.16",
    "graphql": "^14.6.0",
    "graphql-import": "^0.7.1"
  },
  "devDependencies": {
    "@jjangga0214/history-db": "^1.0.0"
  }
}
