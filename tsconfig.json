{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@jjangga0214/*": ["packages/*/src", "services/*/src", "others/*/src"],
      "~communication/*": ["packages/communication/src/*"],
      "~history-db/*": ["packages/history-db/src/*"],
      "~auth/*": ["services/auth/src/*"],
      "~graphql-gateway/*": ["services/graphql-gateway/src/*"],
      "~hasura-transformer/*": ["services/hasura-transformer/src/*"],
      "~history/*": ["services/history/src/*"],
      "~history-trigger/*": ["services/history-trigger/src/*"],
      "~stitching-gateway/*": ["services/stitching-gateway/src/*"],
      "~pulumi/*": ["others/pulumi/src/*"],
      "~*": ["packages/*/src", "services/*/src", "others/*/src"]
    },
    "strict": true,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "incremental": true,
    "resolveJsonModule": true
  }
}
